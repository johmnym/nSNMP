{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "nSNMP": "Debug"
    },
    "Console": {
      "IncludeScopes": true,
      "TimestampFormat": "yyyy-MM-dd HH:mm:ss "
    }
  },
  "SnmpV3": {
    "DefaultTarget": {
      "Host": "192.168.1.1",
      "Port": 161
    },
    "Credentials": {
      "UserName": "admin",
      "AuthProtocol": "SHA256",
      "AuthPassphrase": "your-auth-password-here",
      "PrivProtocol": "AES256",
      "PrivPassphrase": "your-priv-password-here"
    },
    "Operations": {
      "Timeout": "00:00:10",
      "Retries": 3
    },
    "TestOids": [
      "1.3.6.1.2.1.1.1.0",
      "1.3.6.1.2.1.1.3.0",
      "1.3.6.1.2.1.1.5.0",
      "1.3.6.1.2.1.1.6.0"
    ]
  }
}